<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
		<!-- List view -->
		<record id="view_tree_tmp" model="ir.ui.view">
			<field name="name">view_tree_tmp</field>
			<field name="model">vieterp.tmp</field>
			<field name="arch" type="xml">
				<tree string="Module tạm" >
					<field name="name"/>
					<field name="create_date_format"/>
				</tree>
			</field>
		</record>
		
		
		<!-- Search view -->
		<record id="view_search_tmp" model="ir.ui.view">
            <field name="name">view_search_tmp</field>
            <field name="model">vieterp.tmp</field>
            <field name="arch" type="xml">
	            <search string="Module tạm">
					<field name="name"/>					
				</search>
            </field>
        </record>
		
        
        
        <!-- Actions -->
        <record id="action_tmp" model="ir.actions.act_window">
            <field name="name">Module tạm</field>
            <field name="res_model">vieterp.tmp</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
        </record>
	
	</data>
</openerp>