<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	
    	<record id="view_ir_model_tree" model="ir.ui.view">
			<field name="name">view_ir_model_tree</field>
			<field name="model">ir.model</field>
			<field name="arch" type="xml">
				<tree string="ir_model" version="7.0">
					<field name="model"/>				
				</tree>
			</field>
		</record>
		
		<record id="view_ir_model_form" model="ir.ui.view">
		    <field name="name">view_ir_model_form</field>
		    <field name="model">ir.model</field>
		    <field name="type">form</field>
		    <field name="priority" eval="100"/>
		    <field name="arch" type="xml">
		    	<form string="ir_model" version="7.0">
					<sheet>
						<group col="4">	                        
	                        <field name="model"/>
	                    </group>						
						<notebook>
			                <page string="Objects">
			                    <field name="v_object"/>
			                </page>
			            </notebook>		
					</sheet>
                </form>		        
		    </field>
		</record>
        
        <record id="action_ir_model" model="ir.actions.act_window">
            <field name="name">ir_model</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ir.model</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>            
        </record>
        <record id="action_ir_model_tree" model="ir.actions.act_window.view">
            <field eval="100" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_ir_model_tree"/>
            <field name="act_window_id" ref="action_ir_model"/>
        </record>
        <record id="action_ir_model_form" model="ir.actions.act_window.view">
            <field eval="100" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_ir_model_form"/>
            <field name="act_window_id" ref="action_ir_model"/>
        </record>
        
    </data>
</openerp>

