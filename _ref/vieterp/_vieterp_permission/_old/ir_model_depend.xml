<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
		<!-- List view -->
		<record id="view_tree_ir_model_depend" model="ir.ui.view">
			<field name="name">view_tree_ir_model_depend</field>
			<field name="model">ir.model.depend</field>
			<field name="arch" type="xml">
				<tree string="Nhóm đối tượng" version="7.0" editable="top" widget="group_by">
					<field name="name"/>
					<field name="root_model_name"/>
					<field name="count"/>
				</tree>
			</field>
		</record>
		
		
		<!-- Form view -->
		<record id="view_form_ir_model_depend" model="ir.ui.view">
			<field name="name">view_form_ir_model_depend</field>
			<field name="model">ir.model.depend</field>
			<field name="arch" type="xml">
				<form string="Nhóm đối tượng" version="7.0">
					<sheet>
						<group>
							<field name="name"/>
						</group>
					</sheet>		
				</form>
			</field>
		</record>
		
		
		<!-- Search view -->
		<record id="view_search_ir_model_depend" model="ir.ui.view">
            <field name="name">view_search_ir_model_depend</field>
            <field name="model">ir.model.depend</field>
            <field name="arch" type="xml">
	            <search string="Nhóm đối tượng">
					<field name="name"/>
				</search>
            </field>
        </record>
        
        
        <!-- Actions -->
        <record id="action_tree_ir_model_depend" model="ir.actions.act_window">
            <field name="name">Nhóm đối tượng</field>
            <field name="res_model">ir.model.depend</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <!-- <field name="context">{'search_default_group_parent_model': 1}</field> -->
        </record>

    </data>
</openerp>

