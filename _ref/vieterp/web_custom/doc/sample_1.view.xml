							id = 'action_tree_nhan_dinh_ngay_thong_ke_chi_tiet'
							string = 'Chi tiết nhận định ngày'></action>
				</custom>
			</field>
		</record>
		
        <!-- action -->
        
        <record id="action_tree_nhan_dinh_ngay_thong_ke_all" model="ir.actions.act_window">
            <field name="name">Thống kê nhận định ngày</field>
            <field name="res_model">snc.nhan.dinh.ngay.thong.ke</field>
            <field name="view_type">form</field>
            <field name="view_mode">custom</field>
            <field name="view_id" ref="view_tree_nhan_dinh_ngay_thong_ke_all"/>            
        </record>
        
        
	
	</data>
</openerp>