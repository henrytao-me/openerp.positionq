<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		
		<!-- view -->
		
		<record id="view_tree_nhan_dinh_ngay_thong_ke_all" model="ir.ui.view">
			<field name="name">view_tree_nhan_dinh_ngay_thong_ke_all</field>
			<field name="model">snc.nhan.dinh.ngay.thong.ke</field>
			<field name="priority" eval="100"/>
			<field name="arch" type="xml">
				<custom string="Nhận định ngày - tổng hợp" version="7.0"
					template_name = "snc_nhan_dinh_ngay_thong_ke_all"
					template_data = "function_name">
					<action name = "%(action_tree_nhan_dinh_ngay_thong_ke)d" 
							id = 'action_tree_nhan_dinh_ngay_thong_ke'
