<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        <record id="group_snc_goi_thau_dang_tin" model="res.groups">
	        <field name="name">Bài viết (Gói thầu)</field>
	        <field name="category_id" ref="base.module_category_snc"/>
	        <field name="is_public" eval="True"></field>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>	        
	    </record>
	    
	    
	    
	    <record id="category_snc_goi_thau" model="ir.module.category">
    		<field name="name">PTT / Gói thầu</field>
    		<field name="parent_id" ref="base.module_category_ptt"></field>
    	</record>
	    
	    <record id="group_snc_goi_thau_user" model="res.groups">
	        <field name="name">Người dùng</field>
	        <field name="alias">group_snc_goi_thau_user</field>
	        <field name="category_id" ref="category_snc_goi_thau"/>
	        <field name="is_public" eval="True"></field>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="menu_access" eval="[(4, ref('menu_ptt_lgt_vote'))]"></field>	        
	    </record>
	    
	    <record id="group_snc_goi_thau_manager" model="res.groups">
	        <field name="name">Quản trị </field>
	        <field name="alias">group_snc_goi_thau_manager</field>
	        <field name="category_id" ref="category_snc_goi_thau"/>
	        <field name="is_public" eval="True"></field>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="implied_ids" eval="[(4, ref('group_snc_goi_thau_user'))]"/>
	        <field name="menu_access" eval="[(4, ref('menu_ptt_lgt_access_right'))]"></field>	        	        
	    </record>
	    
	    <record id="group_snc_goi_thau_user_test" model="res.groups">
	        <field name="name">Test</field>
	        <field name="alias">group_snc_goi_thau_user_test</field>
	        <field name="category_id" ref="category_snc_goi_thau"/>
	        <field name="is_public" eval="True"></field>
	        <field name="users" eval="[(4, ref('base.user_root'))]"/>
	        <field name="menu_access" eval="[(4, ref('menu_ptt_lgt_vote'))]"></field>	        
	    </record>
        
    </data>
</openerp>