<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	
		<!-- List view -->
		<record id="view_tree_pq_bac" model="ir.ui.view">
			<field name="name">view_tree_pq_bac</field>
			<field name="model">pq.bac</field>
			<field name="arch" type="xml">
				<tree string="Bậc" version="7.0" editable="top">
					<field name="name"/>
				</tree>
			</field>
		</record>
		
		<!-- Form view -->
		<record id="view_form_pq_bac" model="ir.ui.view">
			<field name="name">view_form_pq_bac</field>
			<field name="model">pq.bac</field>
			<field name="arch" type="xml">
				<form string="Bậc" version="7.0">
					<sheet>
						<group colspan="4">
							<field name="name"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		
		<!-- Search view -->
		<record id="view_search_pq_bac" model="ir.ui.view">
            <field name="name">view_search_pq_bac</field>
            <field name="model">pq.bac</field>
            <field name="arch" type="xml">
	            <search string="Bậc" version="7.0">
					<field name="name"/>
				</search>
            </field>
        </record>
        
        <!-- Actions -->
        <record id="act_pq_bac" model="ir.actions.act_window">
            <field name="name">Bậc</field>
            <field name="res_model">pq.bac</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
        </record>
	
	</data>
</openerp>